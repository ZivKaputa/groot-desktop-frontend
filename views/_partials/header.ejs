                        </li>
                        <li class="nav-loc">
                            <a href="/sponsors">Sponsors</a>
                        </li>
                        <% if (!authenticated) { %>
                        <li class="nav-loc">
                            <a href="/join">Join</a>
                        </li>
                        <% } else { %>
                        <li class="nav-loc">
                            <a href="/intranet">Intranet</a>
                        </li>
                        <% } %>
                    </ul>
                </div>
                <div class="top-bar-right">
                    <div class="top-bar-right">
                        <ul class="dropdown menu">
                            <% if (!authenticated) { %>
                            <li>
                                <a id="nav-login" href="/login">
                                    <button id="login" class="button">Login</button>
                                </a>
                            </li>
                            <% } else { %>
                            <li>
                                <a id="nav-login" href="/logout">
                                    <button id="logout" class="button">Logout</button>
                                </a>
                            </li>
                            <% } %>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="mobile">
